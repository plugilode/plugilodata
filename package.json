{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "db:setup": "node --import 'data:text/javascript,import { register } from \"node:module\"; import { pathToFileURL } from \"node:url\"; register(\"ts-node/esm\", pathToFileURL(\"./\"));' scripts/setupDatabase.js",
    "build:server": "tsc -p tsconfig.server.json",
    "server:dev": "cross-env NODE_ENV=development tsx watch src/server.ts",
    "server": "npm run build:server && npm run server:dev",
    "dev:all": "concurrently \"npm run server:dev\" \"npm run dev\"",
    "db:view": "node scripts/viewData.js",
    "db:update-user": "node scripts/updateUser.js",
    "db:update-logos": "node scripts/updateCompanyLogos.js",
    "db:update-urls": "node scripts/updateCompanyUrls.js",
    "db:update-all": "npm run db:update-urls && npm run db:update-logos",
    "db:update-plugilo": "node scripts/updatePlugiloRecord.js",
    "test:db": "cross-env NODE_ENV=development tsx test-mongo-connection.ts",
    "check:users": "cross-env NODE_ENV=development node --import 'data:text/javascript,import { register } from \"node:module\"; import { pathToFileURL } from \"node:url\"; register(\"ts-node/esm\", pathToFileURL(\"./\"));' src/db/check-users.ts",
    "create:user": "cross-env NODE_ENV=development node --import 'data:text/javascript,import { register } from \"node:module\"; import { pathToFileURL } from \"node:url\"; register(\"ts-node/esm\", pathToFileURL(\"./\"));' src/db/create-test-user.ts",
    "check:user": "cross-env NODE_ENV=development tsx scripts/check-user.ts",
    "create:test-user": "cross-env NODE_ENV=development tsx scripts/create-test-user.ts",
    "verify:db": "cross-env NODE_ENV=development tsx scripts/verify-db.ts",
    "check:db": "cross-env NODE_ENV=development tsx scripts/check-db-structure.ts",
    "check:collection": "cross-env NODE_ENV=development tsx scripts/check-collection.ts"
  },
  "dependencies": {
    "@react-three/drei": "^9.99.0",
    "@react-three/fiber": "^8.15.16",
    "@tanstack/react-query": "^5.62.10",
    "@tanstack/react-virtual": "^3.11.2",
    "@types/bcrypt": "^5.0.2",
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^20.11.24",
    "@types/three": "^0.161.2",
    "axios": "^1.6.0",
    "bcrypt": "^5.1.1",
    "better-sqlite3": "^9.4.0",
    "body-parser": "^1.20.3",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "date-fns": "^3.3.1",
    "dotenv": "^16.4.7",
    "emailjs-com": "^3.2.0",
    "express": "^4.21.2",
    "framer-motion": "^11.15.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.294.0",
    "mongodb": "^6.3.0",
    "mongoose": "^8.9.2",
    "natural": "^8.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "socket.io-client": "^4.7.4",
    "three": "^0.161.0",
    "use-debounce": "^10.0.4",
    "zustand": "^4.5.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/better-sqlite3": "^7.6.9",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.11"
  }
}
